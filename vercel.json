{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes":[
        {
            "src": "(.*)",
            "dest": "backend/server.js"
        }
        
    ],
    "env": {
        "DB_URL": "@db_url"
    }
  }
  